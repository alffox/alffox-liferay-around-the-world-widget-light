{
  "sources": ["src"],
  "rules": [
    {
      "test": "\\.json$",
      "exclude": "node_modules",
      "use": ["json-loader"]
    }
  ],
	"create-jar": {
		"output-dir": "dist",
		"features": {
			"js-extender": true,
			"web-context": "/liferay-around-the-world-lite",
			"configuration": "features/configuration.json"
		}
	},
	"dump-report": true
}